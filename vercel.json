{
  "buildCommand": "npm run build",
  "outputDirectory": ".",
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    { "source": "/about.html", "destination": "/about", "permanent": true },
    { "source": "/access.html", "destination": "/access", "permanent": true },
